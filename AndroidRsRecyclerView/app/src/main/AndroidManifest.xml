<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.threehalf.rsrecyclerview.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActListView"></activity>
        <activity android:name=".ActGridView"></activity>
        <activity android:name=".ActStaggeredGridView"></activity>
        <activity android:name=".ActRefreshListView"></activity>
        <activity android:name=".ActRefreshGridView"></activity>
        <activity android:name=".ActRefreshStaggeredGridView"></activity>
    </application>

</manifest>